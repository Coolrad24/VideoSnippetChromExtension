{
    "manifest_version": 2,
    "name": "VideoEditing Downloader",
    "version": "1.0.0",
    
    "content_scripts":[
        {
            "matches": ["<all_urls>"],
            "js":["content.js"]
        }
    ],
    "browser_action":{
        "default_popup":"popup.html"
    },
    "permissions": ["http://localhost/*","activeTab","downloads"]
}